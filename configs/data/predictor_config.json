{
  "model_path": "models/_models/first_model.h5",
  "preprocessor":
  {
    "data_cleaner":
    {
      "name": "BaselineDataCleaner"
    },
    "data_vectorizer":
    {
      "text_vectorizer":
      {
        "type": "embedding",
        "name": "EmbeddingTextVectorizer",
        "embeddings":
        {
          "embedding_type": "glove",
          "embedding_dim": 50,
          "embedding_matrix_path": "preprocessing/_cache/embedding/embedding_matrix.npy",
          "text_encoder":
          {
            "name": "TextEncoder",
            "text_encoder_path": "preprocessing/_cache/embedding/text_encoder.pickle",
            "max_vocab_size": 5000,
            "max_seq_len": 25
          }
        },
        "max_features": 1000,
        "tfidf_vec_path": "preprocessing/_cache/tfidf/vectorizer.vec"
      },
      "output_vectorizer":
      {
        "name": "BasicOutputVectorizer"
      }
    }
  }
}